<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - CHIPS Service Manager</title>
    @await Html.PartialAsync("_BaseStylesPartial")
    <link rel="stylesheet" href="~/css/print.min.css" asp-append-version="true" />
</head>
<body>
    <main class="container">
        @RenderBody()
    </main>
    <footer class="container">
        <hr />
        <p class="text-center">&copy; @DateTime.Now.Year CHIPS</p>
    </footer>
    @await Html.PartialAsync("_BaseScriptsPartial")
    @RenderSection("Scripts", required: false)
</body>
</html>